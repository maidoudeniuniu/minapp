@charset "UTF-8";
/**
 * 下方引入的为uView UI的集成样式文件，为scss预处理器，其中包含了一些"u-"开头的自定义变量
 * uView自定义的css类名和scss变量，均以"u-"开头，不会造成冲突，请放心使用 
 */
.skuBox {
  position: fixed;
  top: 0;
  bottom: 0;
  left: 0;
  bottom: 0;
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  -webkit-box-orient: vertical;
  -webkit-box-direction: normal;
  -webkit-flex-direction: column;
          flex-direction: column;
  width: 100%;
  z-index: 10000;
}
.skuBox .skuBox-hd {
  background: rgba(0, 0, 0, 0.2);
}
.skuBox .googds-sku-info {
  -webkit-box-flex: 1;
  -webkit-flex: 1;
          flex: 1;
  overflow-y: auto;
}
.skuBox .skuBox-sku {
  position: relative;
  background: #ffffff;
  padding: 24rpx;
  padding-bottom: calc(0px + 10px);
  z-index: 10001;
  height: 60vh;
}
.skuBox .skuBox-sku .googds .googds-img {
  height: 220rpx;
  width: 220rpx;
  background: #ffffff;
}
.skuBox .skuBox-sku .googds .googds-info {
  -webkit-box-flex: 1;
  -webkit-flex: 1;
          flex: 1;
}
.skuBox .skuBox-sku .googds .googds-info .googds-close {
  margin-bottom: 90rpx;
}
.skuBox .skuBox-sku .googds .googds-info .googds-close-icon {
  background: url(data:image/png;base64,) center right no-repeat;
  width: 44rpx;
  height: 44rpx;
  background-size: 44rpx auto;
}
.skuBox .skuBox-sku .googds-sku .googds-sku-box {
  -webkit-flex-wrap: wrap;
          flex-wrap: wrap;
}
.skuBox .skuBox-sku .googds-sku .googds-sku-box .sku-item {
  height: 60rpx;
  line-height: 60rpx;
  border: 2rpx solid #dbdce2;
  border-radius: 2rpx;
  padding: 0 20rpx;
  margin-right: 24rpx;
  margin-bottom: 20rpx;
}
.skuBox .skuBox-sku .googds-sku .googds-sku-box .sku-no {
  border: #dbdce2 2rpx dashed;
  color: #dbdce2;
}
.skuBox .skuBox-sku .googds-sku .googds-sku-box .sku-item-ative {
  border: #ff525d 2rpx solid;
  color: #ff525d;
}
.skuBox .skuBox-sku .googds-num {
  position: relative;
  margin-top: 40rpx;
}
.skuBox .skuBox-sku .googds-num .xx-warp-cartNum {
  width: 138rpx;
  height: 60rpx;
  border: 2rpx solid #dbdce2;
  border-radius: 2px;
  position: absolute;
  bottom: 0;
  right: 0;
  z-index: 10;
}
.skuBox .skuBox-sku .googds-num .xx-warp-cartNum .min {
  background: url(data:image/png;base64,) center center no-repeat;
  background-size: 24rpx auto;
  width: 24rpx;
  height: 60rpx;
  margin-left: 12rpx;
}
.skuBox .skuBox-sku .googds-num .xx-warp-cartNum .nomin {
  background: url(data:image/png;base64,) center center no-repeat !important;
  background-size: 24rpx auto !important;
  width: 24rpx;
  height: 60rpx;
  margin-left: 12rpx;
}
.skuBox .skuBox-sku .googds-num .xx-warp-cartNum .cartNum {
  line-height: 60rpx;
  text-align: center;
}
.skuBox .skuBox-sku .googds-num .xx-warp-cartNum .plus {
  background: url(data:image/png;base64,) center center no-repeat;
  background-size: 24rpx auto;
  width: 24rpx;
  height: 60rpx;
  margin-right: 12rpx;
}
.skuBox .skuBox-sku .googds-btn {
  margin-top: 92rpx;
  margin-bottom: 40rpx;
}
.skuBox .skuBox-sku .marking-price {
  font-size: 32rpx;
  color: #b1b2c1;
  line-height: 38rpx;
  margin-left: 6rpx;
  text-decoration: line-through;
}
.buy-button {
  height: 84rpx;
  line-height: 84rpx;
  font-family: PingFangSC-Medium;
  font-size: 28rpx;
  color: #FFFFFF;
  background: #26273A;
  border-radius: 4rpx;
  text-align: center;
}

