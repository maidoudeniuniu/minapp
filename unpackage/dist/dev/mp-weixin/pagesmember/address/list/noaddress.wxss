@charset "UTF-8";
/**
 * 下方引入的为uView UI的集成样式文件，为scss预处理器，其中包含了一些"u-"开头的自定义变量
 * uView自定义的css类名和scss变量，均以"u-"开头，不会造成冲突，请放心使用 
 */
.xx-warp {
  -webkit-box-flex: 1;
  -webkit-flex: 1;
          flex: 1;
  height: 100vh;
}
.xx-warp .xx-warp-hb {
  overflow-y: auto;
  padding: 30rpx;
}
.xx-warp .xx-warp-hb .address-item {
  background: #FFFFFF;
  padding: 24rpx;
  position: relative;
}
.xx-warp .xx-warp-hb .address-item .address-item-icon {
  width: 32rpx;
  height: 32rpx;
  background: url(data:image/png;base64,) center center no-repeat;
  background-size: 32rpx auto;
}
.xx-warp .addressBox {
  padding-top: 304rpx;
  text-align: center;
}
.xx-warp .addressBox .img {
  width: 250rpx;
  height: 250rpx;
  vertical-align: middle;
  margin-top: 25rpx;
}
.xx-warp .addressBox .txt {
  font-family: PingFangSC-Regular;
  font-size: 24rpx;
  color: #26273A;
}
.xx-warp .xx-warp-fb {
  position: absolute;
  width: 750rpx;
  bottom: 0;
  right: 0;
  left: 0;
  padding: 14rpx 0;
}
.add-button {
  width: 702rpx;
  height: 84rpx;
  line-height: 84rpx;
  background: #26273A;
  border-radius: 4rpx;
  font-family: PingFangSC-Medium;
  font-size: 28rpx;
  color: #FFFFFF;
  text-align: center;
  margin: auto;
}

